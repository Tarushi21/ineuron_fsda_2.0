USE DATABASE DEMO_DATABASE;

----1. CONSUMER_COMPLAINT

CREATE OR REPLACE TABLE CONSUMER_COMPLAINTS
    (DATE_RECEIVED STRING,
     PRODUCT_NAME VARCHAR(50),
     SUB_PRODUCT VARCHAR(100),
     ISSUE VARCHAR(100),
     SUBISSUE VARCHAR(100),
     CONSUMER_COMPLAINT_NARRATIVE STRING,
     COMPANY_PUBLIC_RESPONSE STRING,
     COMPANY VARCHAR(100),
     STATE_NAME VARCHAR(4),
     ZIP_CODE STRING,
     TAGS VARCHAR(40),
     CONSUMER_CONSENT_PROVIDED CHAR(25),
     SUBMITTED_VIA STRING,
     DATE_SENT_TO_COMPANY STRING,
     COMPANY_RESPONSE_TO_CONSUMER VARCHAR(40),
     TIMELY_RESPONSE CHAR(4),
     CONSUMER_DISPUTED CHAR(4),
     COMPLAINT_ID NUMBER(10,2) NOT NULL PRIMARY KEY
 );
 
 DESCRIBE TABLE CONSUMER_COMPLAINTS;
 
 SELECT * FROM CONSUMER_COMPLAINTS;

-----2. BRILLIANT_BRITISH_CARS
 
 CREATE OR REPLACE TABLE TS_BRILLIANT_BRITISH_CARS
   (INVOICE_DATE STRING,
    MAKE VARCHAR2(15),
    COUNTRY_NAME VARCHAR(15),
    ISDEALER STRING,
    SALE_PRICE NUMBER(10,2),
    COST_PRICE NUMBER(10,2),
    TOTAL_DISCOUNT NUMBER(5,1),
    DELIVERY_CHARGE INT,
    SPARE_PARTS INT,
    LABOUR_COST NUMBER(10,2),
    CLIENT_NAME VARCHAR2(32),
    MODEL STRING,
    COLOR STRING,
    REPAIRING_YEAR STRING,
    REPORTING_MONTH INT,
    REGISTRATION_DATE STRING,
    VEHICLE_TYPE VARCHAR2(15),
    INVOICE_NUMBER CHAR(40) NOT NULL PRIMARY KEY,
    COUNTRY_ISO_CODE VARCHAR2(5),
    OUTER_POSTODE VARCHAR2(5),
    REGION VARCHAR2(25)
 );
 
 DESCRIBE TABLE TS_BRILLIANT_BRITISH_CARS;
 
 SELECT * FROM TS_BRILLIANT_BRITISH_CARS;
 
 ----3. INSURANCE
 
 CREATE OR REPLACE TABLE INSURANCE
 (ID INT,
 COMPLAIN_DATE STRING,
 COMPLETION_DATE STRING,
 CUSTOMER_ID NUMBER(10) NOT NULL PRIMARY KEY,
 BROKER_ID NUMBER(10),
 PRODUCT_ID INT,
 COMPLAIN_PRIORITY_ID INT,
 COMPLAIN_TYPE_ID INT,
 COMPLAIN_SOURCE_ID INT,
 COMPLAIN_CATEGORY_ID INT,
 COMPLAIN_STATUS_ID INT,
 ADMINISTRATOR_ID INT,
 CLIENT_SATISTFACTION VARCHAR(5),
 EXPECTED_REIMBURSEMENT INT);
  
  DESCRIBE TABLE INSURANCE; 
  
  SELECT * FROM INSURANCE;
  
-------4. SUPER_STORE

CREATE OR REPLACE TABLE TS_SUPER_STORE
  (ROW_ID INT,
  ORDER_ID VARCHAR(15),
  ORDER_DATE DATE,
  SHIP_DATE DATE,
  SHIP_MODE VARCHAR(15),
  CUSTOMER_ID VARCHAR(10),
  CUSTOMER_NAME VARCHAR(30),
  SEGMENT VARCHAR(15),
  COUNTRY_REGION VARCHAR(15),
  CITY VARCHAR(25),
  STATE VARCHAR(25),
  POSTAL_CODE NUMBER(6),
  REGION VARCHAR(7),
  PRODUCT_ID VARCHAR(16),
  CATEGORY VARCHAR(15),
  SUB_CATEGORY VARCHAR(15),
  PRODUCT_NAME VARCHAR(130),
  SALES FLOAT,
  QUANTITY INT,
  DISCOUNT NUMBER(3,2),
  PROFIT FLOAT);


DESCRIBE TABLE TS_SUPER_STORE;
SELECT * FROM TS_SUPER_STORE;

----5. SALES DATA

CREATE OR REPLACE TABLE TS_SALES_DATA
  ( INVOICE_DATE STRING,
    MAKE VARCHAR(15),
    COUNTRY_NAME VARCHAR(16),
    ISDEALER STRING,
    SALE_PRICE NUMBER(10,2),
    COST_PRICE NUMBER(10,2),
    TOTAL_DISCOUNT INT,
    DELIVERY_CHARGE INT,
    SPARE_PARTS INT,
    LABOR_COST INT,
    CLIENT_NAME VARCHAR(35),
    MODEL VARCHAR(15),
    COLOR VARCHAR(21),
    REPORTING_YEAR STRING,
    REPORTING_MONTH INT,
    REGISTRATION_DATE DATE,
    VEHICLE_TYPE VARCHAR(15),
    INVOICE_NO VARCHAR(50));


DESCRIBE TABLE TS_SALES_DATA;
SELECT * FROM TS_SALES_DATA;

-----6. UK_BANK_CUSTOMERS

CREATE OR REPLACE TABLE TS_UK_BANK_CUSTOMERS
(CUSTOMER_ID NUMBER(10),
 NAME VARCHAR(30),
 SURNAMR VARCHAR(30),
 GENDER VARCHAR(7),
 AGE INT,
 REGION VARCHAR(20),
 JOB_CLASSIFICATION VARCHAR(15),
 DATE DATE,
 BALANCE FLOAT);
 
SELECT * FROM TS_UK_BANK_CUSTOMERS;
